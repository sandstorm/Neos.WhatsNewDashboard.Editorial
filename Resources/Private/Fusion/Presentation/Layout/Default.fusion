prototype(Flowpack.Neos.WhatsNewDashboard.Editorial:Layout.Default) < prototype(Neos.Fusion:Component) {
    content = ''

    @propTypes {
        @strict = true
        content = ${PropTypes.string}
    }

    renderer = afx`
        <main id="mainContent">
            {props.content}
        </main>
    `
}
