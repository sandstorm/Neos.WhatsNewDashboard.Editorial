prototype(Flowpack.Neos.WhatsNewDashboard.Editorial:Component.Image.Description) < prototype(Neos.Fusion:Component) {
    imageDescription = ''

    @propTypes {
        @strict = true
        imageDescription = ${PropTypes.string}
    }

    renderer = afx`
        <span @if.imageDescription={props.imageDescription} class="block text-left text-white mt-4">{props.imageDescription}</span>
    `
}
