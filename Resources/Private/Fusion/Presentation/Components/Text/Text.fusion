prototype(Flowpack.Neos.WhatsNewDashboard.Editorial:Component.Text) < prototype(Neos.Fusion:Component) {
    text = ''

    @propTypes {
        @strict = true
        text = ${PropTypes.string}
    }
    renderer = afx`
        <div class="max-w-md">
            {props.text}
        </div>
    `
}
